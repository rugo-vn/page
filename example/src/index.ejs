<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Simple EJS page</title>
    <%- await include('./parts/header.ejs') %>
  </head>
  <body class="p-4 font-mono">
    <%- await include('./parts/nav.ejs') %> <% for (let i = 0; i < 10; i++) { %>
    <div class="text-xs tracking-widest inline">Hello <%= i %></div>
    <br />
    <% } %> <%= JSON.stringify(await table('posts').find({})) %> <%=
    JSON.stringify(await table('posts').get({ id: 1})) %>
    <div>Now is: <%= moment().format('YYYY/MM/DD HH:mm:ss') %></div>
    <div><%= (await http.get('http://example.com')).data %></div>
    <div><%= params.name %></div>
    <%- await include('./parts/footer.ejs') %>
  </body>
</html>
